{
  "ConnectionStrings": {
    "DefaultConnection": "#{ConnectionString}"
  },
  "TokenSettings": {
    "Secret": "#{TokenSecret}",
    "Expires": "#{TokenExpires}",
    "Issuer": "#{TokenIssuer}",
    "Audience": "#{TokenAudience}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
